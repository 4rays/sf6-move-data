{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "name": {
      "type": "string"
    },
    "name_ja": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "abbreviation": {
      "type": "string"
    },
    "abbeviation_ja": {
      "type": "string"
    },
    "characterId": {
      "type": "integer"
    },
    "type": {
      "$ref": "#/definitions/Type"
    },
    "damage": {
      "$ref": "#/definitions/Damage"
    },
    "hitCount": {
      "type": "integer"
    },
    "blockType": {
      "$ref": "#/definitions/BlockType"
    },
    "input": {
      "type": "string"
    },
    "cancel": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Cancel"
      }
    },
    "chainsInto": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "frameAdvantage": {
      "$ref": "#/definitions/FrameAdvantage"
    },
    "notes": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "notes_ja": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "properties": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "parentSlug": {
      "type": "string"
    },
    "alternativeNames": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "invincibilityFrames": {
      "$ref": "#/definitions/InvincibilityFrames"
    }
  },
  "required": [
    "characterId",
    "hitCount",
    "input",
    "name",
    "name_ja",
    "slug",
    "type"
  ],
  "title": "Move",
  "definitions": {
    "FrameAdvantage": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "hit": {
          "type": "integer"
        },
        "block": {
          "type": "integer"
        }
      },
      "required": ["hit"],
      "title": "FrameAdvantage"
    },
    "InvincibilityFrames": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "airStart": {
          "type": "integer"
        },
        "airEnd": {
          "type": "integer"
        },
        "start": {
          "type": "integer"
        },
        "end": {
          "type": "integer"
        },
        "strikeStart": {
          "type": "integer"
        },
        "strikeEnd": {
          "type": "integer"
        }
      },
      "required": [],
      "title": "InvincibilityFrames"
    },
    "Damage": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "string",
          "format": "integer"
        }
      ],
      "title": "Damage"
    },
    "Cancel": {
      "type": "string",
      "enum": [
        "C",
        "SA",
        "SA2",  
        "SA3",
        "J",
        "*"
      ],
      "title": "Cancel"
    },
    "BlockType": {
      "type": "string",
      "enum": ["high", "low", "mid"],
      "title": "BlockType"
    },
    "Type": {
      "type": "string",
      "enum": [
        "normal",
        "commandNormal",
        "targetCombo",
        "special",
        "super1",
        "super2",
        "super3",
        "throw"
      ],
      "title": "Type"
    }
  }
}
